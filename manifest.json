{
  "manifest_version": 3,
  "name": "Sent URL to Slack",
  "version": "1.0.0",
  "description": "現在開いているページのURLを特定のSlackチャンネルに投稿する",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "/popup/index.html"
  }
}